{
  "version": "1",
  "cognitoConfig": {
    "identityPoolName": "complimentdeckIdentityPool",
    "allowUnauthenticatedIdentities": false,
    "resourceNameTruncated": "compli",
    "userPoolName": "complimentdeck-users",
    "autoVerifiedAttributes": ["email"],
    "mfaConfiguration": "OFF",
    "mfaTypes": ["SMS Text Message"],
    "smsAuthenticationMessage": "Your verification code is {####}",
    "smsVerificationMessage": "Your verification code is {####}",
    "emailVerificationSubject": "Your Compliment Deck verification code",
    "emailVerificationMessage": "Your verification code is {####}",
    "defaultPasswordPolicy": true,
    "passwordPolicyMinLength": 8,
    "passwordPolicyCharacters": [],
    "requiredAttributes": ["email"],
    "aliasAttributes": [],
    "userpoolClientGenerateSecret": false,
    "userpoolClientRefreshTokenValidity": 30,
    "userpoolClientWriteAttributes": ["email"],
    "userpoolClientReadAttributes": ["email"],
    "userpoolClientLambdaRole": "compli_userpoolclient_lambda_role",
    "userpoolClientSetAttributes": false,
    "sharedId": "auth",
    "resourceName": "complimentdeckAuth",
    "authSelections": "userPoolOnly",
    "useDefault": "default",
    "userPoolGroups": false,
    "adminQueries": false,
    "triggers": {},
    "hostedUI": false,
    "userPoolGroupList": [],
    "serviceName": "Cognito",
    "usernameCaseSensitive": false,
    "useEnabledMfas": true,
    "authRoleArn": {
      "Fn::GetAtt": ["AuthRole", "Arn"]
    },
    "unauthRoleArn": {
      "Fn::GetAtt": ["UnauthRole", "Arn"]
    },
    "breakCircularDependency": true,
    "dependsOn": [],
    "thirdPartyAuth": false,
    "authProviders": []
  }
}
